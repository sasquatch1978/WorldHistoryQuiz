    <string name="questionTitle4">Question 4 of 10:</string>
    <string name="question4">Which two cities were destroyed in the Mount Vesuvius eruption of AD 79?</string>
    <string name="answer4A">A) Herculaneum</string>
    <string name="answer4B">B) Misenum</string>
    <string name="answer4C">C) Pompeii</string>
    <string name="answer4D">D) Neapolis</string>
