    <!-- Question one. [CHAR LIMIT=NONE] -->
    <string name="questionTitle1">Question 1 of 10:</string>
    <string name="question1">An early form of writing developed by the Sumerians was called:</string>
    <string name="answer1A">A) Sanskrit</string>
    <string name="answer1B">B) Cuneiform</string>
    <string name="answer1C">C) Indus</string>
    <string name="answer1D">D) Elamite</string>
    <!-- Question two. [CHAR LIMIT=NONE] -->
    <string name="questionTitle2">Question 2 of 10:</string>
    <string name="question2">The first Olympic Games in 776 BC, were held in what Greek city?</string>
    <string name="answer2A">A) Athens</string>
    <string name="answer2B">B) Corinth</string>
    <string name="answer2C">C) Olympia</string>
    <string name="answer2D">D) Thebes</string>
    <!-- Question three. [CHAR LIMIT=NONE] -->
    <string name="questionTitle3">Question 3 of 10:</string>
    <string name="question3">The Great Pyramid, Egypt\'s largest pyramid, is located where?</string>
    <string name="answer3A">A) Giza</string>
    <string name="answer3B">B) Dahshur</string>
    <string name="answer3C">C) Lisht</string>
    <string name="answer3D">D) Nuri</string>
