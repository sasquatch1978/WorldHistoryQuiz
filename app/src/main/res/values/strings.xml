    <string name="questionTitle5">Question 5 of 10:</string>
    <string name="question5">What was the Colosseum in Rome originally used for?</string>
    <string name="answer5A">A) Housing</string>
    <string name="answer5B">B) Entertainment</string>
    <string name="answer5C">C) Workshops</string>
    <string name="answer5D">D) Fortress</string>
